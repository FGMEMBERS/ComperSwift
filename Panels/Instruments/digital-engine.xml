<?xml version="1.0"?>

<!--
-->

<PropertyList>

 <name>Digital Engine monitor display</name>
 <w-base>40</w-base>
 <h-base>10</h-base>

 <params>
  <pt-select>/engines/engine[0]/prop-thrust</pt-select>
  <ff-select>/engines/engine[0]/fuel-flow-gph</ff-select>
  <rv-select>/engines/engine[0]/rpm</rv-select>
  <th-select>/controls/engines/engine[0]/throttle</th-select>
 </params>

 <layers>

  <layer>
   <name>background</name>
   <texture>
    <path>Aircraft/ComperSwift/Panels/Instruments/Textures/trans-orange-bg.rgb</path>
   </texture>
    <w>380</w>
    <h>40</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>190</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>0</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Th-Title</name>
   <type>text</type>
   <point-size>18</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>Th: </text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>0</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Th display</name>
   <type>text</type>
   <font>led</font>
   <point-size>7</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property alias="../../../../../params/th-select"/>
     <format>%01.3f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>60</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>4</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>PT-Title</name>
   <type>text</type>
   <point-size>18</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>PT: </text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>130</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>0</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Prop Thrust display</name>
   <type>text</type>
   <font>led</font>
   <point-size>7</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property alias="../../../../../params/pt-select"/>
     <format>%04.1f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>170</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>4</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>RPM-Title</name>
   <type>text</type>
   <point-size>18</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>RPM: </text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-20</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>RPM display</name>
   <type>text</type>
   <font>led</font>
   <point-size>7</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property alias="../../../../../params/rv-select"/>
     <format>%05.f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>60</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-16</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>FF-Title</name>
   <type>text</type>
   <point-size>18</point-size>
   <color>
    <red>0.5</red>
    <green>0.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>FF: </text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>260</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-20</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>FF display</name>
   <type>text</type>
   <font>led</font>
   <point-size>7</point-size>
   <color>
    <red>0.5</red>
    <green>0.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property alias="../../../../../params/ff-select"/>
     <format>%02.3f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>320</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-16</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>EGT-Title</name>
   <type>text</type>
   <point-size>18</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>EGT: </text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>260</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>0</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>EGT display</name>
   <type>text</type>
   <font>led</font>
   <point-size>7</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property alias="../../../../../params/egt-select"/>
     <format>%04.0f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>320</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>4</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

</PropertyList>
